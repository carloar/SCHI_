                    <CommandText>SELECT        empleados.ficha_emp, empleados.nombre, empleados.apellido, empleados.departamento, herramientas.cod_herr, herramientas.descripcion, 
                         herramientas.tipo_herr, prestamo.cantidad, prestamo.estatus, prestamo.fecha
